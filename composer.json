{
    "name": "datana-gmbh/project-name",
    "description": "A description here",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-pdo_pgsql": "*",
        "api-platform/core": "^3.2.4",
