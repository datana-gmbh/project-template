        "doctrine/migrations": "^3.6.0",
        "doctrine/orm": "^2.16.2",
        "doctrine/persistence": "^2.5.7",
        "easycorp/easyadmin-bundle": "^4.8.4",
        "nelmio/cors-bundle": "^2.3.1",
        "oskarstark/symfony-http-responder": "^1.1",
        "oskarstark/trimmed-non-empty-string": "^1.0",
        "psr/log": "^3.0",
        "symfony/asset": "^6.3",
        "symfony/console": "^6.3.4",
        "symfony/dependency-injection": "^6.3.5",
        "symfony/doctrine-bridge": "^6.3.7",
        "symfony/dotenv": "^6.3.7",
        "symfony/event-dispatcher": "^6.3.2",
        "symfony/expression-language": "^6.3",
        "symfony/finder": "^6.3.5",
        "symfony/flex": "^2.4.1",
        "symfony/framework-bundle": "^6.3.7",
        "symfony/http-foundation": "^6.3.7",
        "symfony/http-kernel": "^6.3.7",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^6.3.2",
        "symfony/property-info": "^6.3",
        "symfony/routing": "^6.3.5",
        "symfony/security-bundle": "^6.3.7",
        "symfony/security-core": "^6.3.7",
        "symfony/security-http": "^6.3.6",
        "symfony/serializer": "^6.3.7",
        "symfony/twig-bridge": "^6.3.5",
        "symfony/twig-bundle": "^6.3",
        "symfony/uid": "^6.3",
        "symfony/validator": "^6.3.7",
        "symfony/var-dumper": "^6.3.6",
        "symfony/yaml": "^6.3.7",
        "thecodingmachine/safe": "^2.5",
        "twig/twig": "^3.7.1",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.2.1",
        "datana-gmbh/doctrine-type-testcases": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4.4",
        "ergebnis/composer-normalize": "^2.39.0",
        "ergebnis/php-cs-fixer-config": "^6.11",
        "ergebnis/test-util": "^1.6",
        "fakerphp/faker": "^1.23",
        "icanhazstring/composer-unused": "^0.8.10",
        "infection/infection": "^0.26.21",
        "mtdowling/jmespath.php": "^2.7.0",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.41",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-doctrine": "^1.3.46",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpstan/phpstan-strict-rules": "^1.5.2",
        "phpstan/phpstan-symfony": "^1.3.5",
        "phpstan/phpstan-webmozart-assert": "^1.2.4",
        "phpunit/phpunit": "^9.6.13",
        "rector/rector": "^0.18.6",
        "symfony/browser-kit": "^6.3.2",
        "symfony/css-selector": "^6.3.2",
        "symfony/debug-bundle": "^6.3.2",
        "symfony/maker-bundle": "^1.51.1",
        "symfony/phpunit-bridge": "^6.3.6",
        "symfony/web-profiler-bundle": "^6.3.6",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "vincentlanglet/twig-cs-fixer": "^1.6.0",
        "zenstruck/browser": "^1.6",
        "zenstruck/console-test": "^1.5",
        "zenstruck/foundry": "^1.36.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true
        },
        "optimize-autoloader": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
